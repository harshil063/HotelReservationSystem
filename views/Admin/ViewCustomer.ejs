<%- include('../partials/Admin/header.ejs') %>
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">All Room Categories</h3>
                    </div>
                    
                    <!-- /.card-header -->
                    <div class="card-body">
                            <table id="example2" class="table table-bordered table-hover">
                                <tr>
                                    <th>Sr No.</th>
                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>Email</th>
                                    <th>Contact</th>
                                    <th>AadharCard Number</th>
                                    <th>City</th>
                                    <th>Pincode</th>
                                </tr>

                                <% var i = 1 %>
                                <% data.forEach((item)=>{ %>
                                    <tr>
                                        <td><%= i++ %></td>
                                        <td><%= item.fname %></td>
                                        <td><%= item.lname %></td>
                                        <td><%= item.email %></td>
                                        <td><%= item.contact  %></td>
                                        <td><%= item.aadharcardno %></td>
                                        <td><%= item.city%></td>
                                        <td><%= item.pincode %></td>
                                    </tr>
                                <% }) %>

                                <tfoot>
                                    <tr>
                                        <th>Sr No.</th>
                                        <th>First Name</th>
                                        <th>Last Name</th>
                                        <th>Email</th>
                                        <th>Contact</th>
                                        <th>AadharCard Number</th>
                                        <th>City</th>
                                        <th>Pincode</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    <!-- /.card-body -->
                </div>
                
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>
<%- include('../partials/Admin/footer.ejs') %>